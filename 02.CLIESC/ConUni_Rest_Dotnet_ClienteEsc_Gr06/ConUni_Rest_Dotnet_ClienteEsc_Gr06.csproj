  <ItemGroup>
    <None Remove="monster2.jpg" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="monster2.jpg">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>

</Project>